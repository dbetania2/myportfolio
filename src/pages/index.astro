---
import Character from '../components/character/Character.astro'; // Ruta al componente 
import Cama from '../objetos-img/cama.png';
import Pc from '../objetos-img/pc.png';
---

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Mapa PNG</title>
  <link rel="stylesheet" href="/src/pages/index.css">
</head>
<body>
  <div id="map-container">
    <img src="/assets/portfolioroom.png" alt="Mapa" id="map-image">
    <img src={Cama.src} alt="cama" class="map-object" id="cama" />
    <img src={Pc.src} alt="pc" class="map-object" id="pc" />

    <Character />
  </div>
</body>
</html>





